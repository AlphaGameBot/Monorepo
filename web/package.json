{
  "name": "alphagamebot-webui",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "postversion": "git add . && git commit -am \"WebUI Release webui-v$(cat package.json | jq .version -cMr)\" && git tag -a \"webui-v$(cat package.json | jq .version -cMr)\" -m \"WebUI Release $(cat package.json | jq .version -cMr)\"",
    "dev": "set NODE_OPTIONS='--env-file=.env' && next dev -p 4000",
    "prebuild": "node scripts/generate-humans.js",
    "postbuild": "next-sitemap",
    "predev": "node scripts/generate-humans.js",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --config ../eslint.config.ts ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@prisma/client": "^6.19.0",
    "discord.js": "^14.25.1",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "markdown-to-txt": "^2.0.1",
    "marked": "^4.3.0",
    "next": "^16.0.7",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@prisma/config": "^6.19.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.2",
    "next-sitemap": "^4.2.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
